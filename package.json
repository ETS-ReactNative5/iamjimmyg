{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-scrollable-anchor": "^0.6.1",
    "react-sticky": "^6.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar public/css/ -o public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar public/css/ -o public/css/ --watch --recursive",
